{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "cdmsdatafact"
		},
		"AzureBatch1_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'AzureBatch1'"
		},
		"AzureBatch2_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'AzureBatch2'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureFunction1_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'AzureFunction1'"
		},
		"CosmosDbMongoDbApi1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbMongoDbApi1'"
		},
		"CosmosDbMongoDbApi2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbMongoDbApi2'"
		},
		"linkedService1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linkedService1'"
		},
		"FileShare1_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "Sample1.csv"
		},
		"FileShare1_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "source/D2"
		},
		"SourceDataset_0al_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "SampleDaily_20181207.csv"
		},
		"SourceDataset_0al_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "cdmscontainer"
		},
		"SourceDataset_r2p_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "SampleDaily_20181207.csv"
		},
		"SourceDataset_r2p_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "cdmscontainer"
		},
		"SourceDataset_xo4_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "SampleDaily_20190111_1.txt"
		},
		"SourceDataset_xo4_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "cdmscontainer"
		},
		"AzureBatch1_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "cdmsbatch"
		},
		"AzureBatch1_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://cdmsbatch.centralus.batch.azure.com"
		},
		"AzureBatch2_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "cdmsbatch"
		},
		"AzureBatch2_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://cdmsbatch.centralus.batch.azure.com"
		},
		"CosmosDbMongoDbApi1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "cdmsDB"
		},
		"CosmosDbMongoDbApi2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "cdmsDB"
		},
		"linkedService1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "cdmsstorageaccnt"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureFunction1_functionKey": {
						"value": "[parameters('AzureFunction1_functionKey')]"
					},
					"CosmosDbMongoDbApi1_connectionString": {
						"value": "[parameters('CosmosDbMongoDbApi1_connectionString')]"
					},
					"CosmosDbMongoDbApi2_connectionString": {
						"value": "[parameters('CosmosDbMongoDbApi2_connectionString')]"
					},
					"linkedService1_password": {
						"value": "[parameters('linkedService1_password')]"
					},
					"AzureBatch1_accessKey": {
						"value": "[parameters('AzureBatch1_accessKey')]"
					},
					"AzureBatch2_accessKey": {
						"value": "[parameters('AzureBatch2_accessKey')]"
					},
					"CosmosDbMongoDbApi1_properties_typeProperties_database": {
						"value": "[parameters('CosmosDbMongoDbApi1_properties_typeProperties_database')]"
					},
					"CosmosDbMongoDbApi2_properties_typeProperties_database": {
						"value": "[parameters('CosmosDbMongoDbApi2_properties_typeProperties_database')]"
					},
					"linkedService1_properties_typeProperties_userId": {
						"value": "[parameters('linkedService1_properties_typeProperties_userId')]"
					},
					"FileShare1_properties_typeProperties_fileName": {
						"value": "[parameters('FileShare1_properties_typeProperties_fileName')]"
					},
					"FileShare1_properties_typeProperties_folderPath": {
						"value": "[parameters('FileShare1_properties_typeProperties_folderPath')]"
					},
					"SourceDataset_0al_properties_typeProperties_fileName": {
						"value": "[parameters('SourceDataset_0al_properties_typeProperties_fileName')]"
					},
					"SourceDataset_0al_properties_typeProperties_folderPath": {
						"value": "[parameters('SourceDataset_0al_properties_typeProperties_folderPath')]"
					},
					"SourceDataset_r2p_properties_typeProperties_fileName": {
						"value": "[parameters('SourceDataset_r2p_properties_typeProperties_fileName')]"
					},
					"SourceDataset_r2p_properties_typeProperties_folderPath": {
						"value": "[parameters('SourceDataset_r2p_properties_typeProperties_folderPath')]"
					},
					"SourceDataset_xo4_properties_typeProperties_fileName": {
						"value": "[parameters('SourceDataset_xo4_properties_typeProperties_fileName')]"
					},
					"SourceDataset_xo4_properties_typeProperties_folderPath": {
						"value": "[parameters('SourceDataset_xo4_properties_typeProperties_folderPath')]"
					},
					"AzureBatch1_properties_typeProperties_accountName": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_accountName')]"
					},
					"AzureBatch1_properties_typeProperties_batchUri": {
						"value": "[parameters('AzureBatch1_properties_typeProperties_batchUri')]"
					},
					"AzureBatch2_properties_typeProperties_accountName": {
						"value": "[parameters('AzureBatch2_properties_typeProperties_accountName')]"
					},
					"AzureBatch2_properties_typeProperties_batchUri": {
						"value": "[parameters('AzureBatch2_properties_typeProperties_batchUri')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}